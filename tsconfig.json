{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "lib/StoreProvider.ts", "lib/fetchFunctions/fetchData.js", "lib/hooks.ts", "lib/hooks.ts", "lib/store.ts", "lib/reducers/cartSlice.ts", "app/components/search/SearchBar.tsx", "components/HMenu/HMenu.js", "app/page.js", "components/SIgnIn_Google.js", "components/home/cards/CardItem.js", "components/home/cards/Card.js", "components/home/slider/Slider.js"],
  "exclude": ["node_modules"]
}
